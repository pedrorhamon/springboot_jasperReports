	<queryString language="SQL">
		<![CDATA[SELECT jaspercurso.empresas.razao_social,
	jaspercurso.empresas.cnpj,
	jaspercurso.empresas.telefone,
	jaspercurso.empresas.email,
	jaspercurso.funcionarios.id_funcionario,
	jaspercurso.funcionarios.nome,
	concat( jaspercurso.enderecos.logradouro,', ',jaspercurso.enderecos.numero,' - ',jaspercurso.enderecos.bairro,', ',jaspercurso.enderecos.cidade,'/',jaspercurso.enderecos.uf) 
	AS endereco
